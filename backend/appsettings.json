{
	"Logging": {
		"LogLevel": {
			"Default": "Trace",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"ApplicationInsights": {},
	"RateLimiting": {
		"GlobalLimiter": {
			"AuthenticatedUserLimit": 200,
			"AnonymousUserLimit": 50,
			"AuthenticatedUserQueueLimit": 10,
			"AnonymousUserQueueLimit": 2,
			"WindowMinutes": 1
		},
		"ActionItems": {
			"AuthenticatedUserLimit": 300,
			"AnonymousUserLimit": 30,
			"AuthenticatedUserQueueLimit": 20,
			"AnonymousUserQueueLimit": 5,
			"WindowMinutes": 1
		},
		"AI": {
			"AuthenticatedUserLimit": 20,
			"AnonymousUserLimit": 5,
			"AuthenticatedUserQueueLimit": 5,
			"AnonymousUserQueueLimit": 1,
			"WindowMinutes": 1
		},
		"Auth": {
			"UserLimit": 10,
			"QueueLimit": 2,
			"WindowMinutes": 1
		}
	}
}
